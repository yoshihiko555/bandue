    "rules": {
      "no-tabs": 0,
      "indent": "off",
      "no-unused-vars": "off",
      "no-mixed-spaces-and-tabs": "off",
      "quotes": "off",
      "comma-dangle": "off",
      "quote-props": "off",
      "object-curly-spacing": "off",
      "key-spacing": "off",
      "space-before-function-paren": "off",
      "space-before-blocks": "off",
      "no-trailing-spaces": "off",
      "comma-spacing": "off",
      "padded-blocks": "off",
      "prefer-const": "off",
      "no-unused-components": "off",
      "no-multi-spaces": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
